{
	"name": "nella/monolog-tracy-bundle",
	"description": "Tracy BlueScreen handler for Monolog - Symfony bundle",
	"type": "library",
	"keywords": ["Monolog", "Tracy", "log", "logging", "blue screen", "Symfony", "bundle"],
	"homepage": "https://github.com/pavelkucera/monolog-extensions-bundle",
	"license": ["MIT", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Pavel Kučera",
			"homepage": "https://github.com/pavelkucera"
		},
		{
			"name": "Patrik Votoček",
			"email": "patrik@votocek.cz",
			"homepage": "https://patrik.votocek.cz"
		}
	],
	"support": {
		"issues": "https://github.com/nella/monolog-tracy/issues",
		"source": "https://github.com/nella/monolog-tracy"
	},
	"autoload": {
		"psr-4": {
			"Nella\\MonologTracy\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Nella\\MonologTracy\\": "tests/"
		}
	},
	"require": {
		"php": "~5.6|~7.0",
		"kucera/monolog-extensions": "~0.1.0",
		"symfony/config": "~2.6|~3.0",
		"symfony/dependency-injection": "~2.6|~3.0",
		"symfony/monolog-bundle": "~2.6|~3.0",
		"symfony/yaml": "~2.6|~3.0"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"nella/coding-standard": "4.1.0",
		"phpunit/phpunit": "5.1.3"
	},
	"config": {
		"bin-dir": "bin"
	},
	"archive": {
		"exclude": [
			".gitattributes",
			".gitignore",
			"/build",
			"/tests"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
